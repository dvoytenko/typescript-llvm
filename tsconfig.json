{
  "compilerOptions": {
    // "module": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "out",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": [
      "node",
    ],
    "jsx": "react",
    "jsxFactory": "jsx",
  },
  "watchOptions": {
    "excludeDirectories": ["out"]
  },
  "lib": ["es2020"]
}
